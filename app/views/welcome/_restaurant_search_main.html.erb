<div class="container">
  <div class="col-md-4 logo-side">
    <img class="logo-image" alt="Smiley face" src="./assets/logo.png">
  </div>
  <div class="col-md-8 add-side">
    <img class="logo-image" alt="Smiley face" src="./assets/add.png">
  </div>
  <div class="row school-campus">
    <div class="col-md-10 col-md-offset-1">
      <p class="school-name">Enter your School name or Campus to find local restaurants!</p>
      <div class="main-search-bar">
        <%= form_for "search", :url => result_welcome_index_path, :method => 'get'  do |f| %>
          <div class="search-botton">
            <div class="delivery"><%= check_box_tag :is_delivery, true %>DELIVERY</div>
            <div class="pickup"><%= check_box_tag :is_pick_up, true %>PICKUP</div>
          </div>
          <div class="search-field">
            <input type="text" name = "keyword",  placeholder="School Name or Campus", class = "search-keyword", value = <%= params[:keyword] %> >
          </div>
          <div class="search">
            <button class="search-b">Search</button>
          </div>
        <% end -%>  
      </div>
    </div>
  </div>
  <div class="row school-campus-img">
    <div class="col-md-10 col-md-offset-1 padding-zero">
      <img class="logo-image" alt="Smiley face" src="./assets/liner.png" />
    </div>
  </div>
</div>