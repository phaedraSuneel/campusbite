<div class="row search-container result-page menupage checkout-page last-page aboutus-page">
  <div class="container">
    <div class="col-md-12 main-fram-container">
      <div class="row">
        <div class="col-md-4  col-md-offset-4 about-logo">
            <%= link_to raw("<img class='logo-image' alt='Smiley face' src='/assets/about-logo.png'>"), '/' %>
        </div>
        <div class="col-md-4 house-logo-1">
          <img class="logo-image" alt="Smiley face" src="/assets/LastPage_22.png" />
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 about-heading padding-zero">
          <h1>frequently asked questions</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 main-fram-canvus">
          <div class="row">
            <div class="col-md-12 about-heading faqpage-questions">
              <% @faqs.each do |f| %>
                <div class="faq-question">
                  <h2><%= f.question %></h2>
                  <p> <%= f.answer %></p>
                </div>
              <% end -%>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>