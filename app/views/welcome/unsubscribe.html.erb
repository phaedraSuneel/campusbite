<div class="row search-container result-page menupage checkout-page last-page aboutus-page career-page">
  <div class="container">
    <div class="col-md-12 main-fram-container">
      <div class="row">
        <div class="col-md-4  col-md-offset-4 about-logo">
          <%= link_to raw("<img class='logo-image' alt='Smiley face' src='/assets/about-logo.png'>"), '/' %>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 about-heading">
          <h1>unsubscribe</h1>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 about-heading">
          <div class="unsubscribe">
            <h4>Unsubscribe if you no longer wish to receive email updates from Campus Bite</h4>
            <%= form_for(:subscriber, :url => remove_subscriber_welcome_index_path ) do |f| %>
              <label>Email:</label>
              <%= f.text_field :email %>
              <div class="unsubscribe-btn"><button>unsubscribe</button>
            <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>