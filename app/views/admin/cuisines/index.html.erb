<h1>Listing cuisines</h1>

<table class="table table-striped user-result">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cuisines.each do |cuisine| %>
  <tr>
    <td><%= cuisine.name %></td>
    <td><%= cuisine.description %></td>
    <td><%= link_to 'Show', admin_cuisine_path(cuisine) %></td>
    <td><%= link_to 'Edit', edit_admin_cuisine_path(cuisine) %></td>
    <td><%= link_to 'Destroy', admin_cuisine_path(cuisine), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cuisine', new_admin_cuisine_path %>
