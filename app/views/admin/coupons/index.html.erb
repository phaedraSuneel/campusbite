<h1>Listing coupons</h1>
<div class="row">
  <div class="col-md-12">
    <h3 class="page-title">
     Coupons
    </h3>
    <ul class="page-breadcrumb breadcrumb">
      <li>
        <i class="fa fa-home"></i>
        <%= link_to "Home", admin_dashboard_index_path %>
        <i class="fa fa-angle-right"></i>
      </li>
      <li>
        <%= link_to "Coupons", admin_coupons_path %>
      </li>
    </ul>  
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="portlet">
      <div class="portlet-title">
        <div class="caption">
          <i class="fa fa-shopping-cart"></i>Listing coupons
        </div>
        <div class="pull-right">
          <%= link_to 'New Coupon', new_admin_coupon_path, :class => "btn btn-primary" %>
        </div>
      </div>
      <div class="portlet-body">
        <div class="table-container">
          <table class="table table-striped table-bordered table-hover" id="coupons-table">
            <thead>
              <tr>
                <th>Code</th>
                <th>Amount</th>
                <th>Max user</th>
                <th>Max use</th>
                <th></th>
                <th></th>
                <th></th>
              </tr>  
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>             
