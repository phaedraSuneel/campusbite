<h1>Listing redeams</h1>

<table>
  <tr>
    <th>Item</th>
    <th>Item name</th>
    <th>Item price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @redeams.each do |redeam| %>
  <tr>
    <td><%= redeam.item_id %></td>
    <td><%= redeam.item_name %></td>
    <td><%= redeam.item_price %></td>
    <td><%= link_to 'Show', redeam %></td>
    <td><%= link_to 'Edit', edit_redeam_path(redeam) %></td>
    <td><%= link_to 'Destroy', redeam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Redeam', new_redeam_path %>
